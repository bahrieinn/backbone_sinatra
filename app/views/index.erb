<div class="container">
  <h1>A backbone + sinatra app</h1>
  <hr />
  <div class="page"></div>
</div>

<script type="text/template" id="user-list-template">
  <table class="table striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      {{ _.each(users, function(user){ }}
      <tr>
        {{ var user = user.get('user') }}
        <td>{{= user.first_name }}</td>
        <td>{{= user.last_name }}</td>
        <td>{{= user.age }}</td>
      </tr>
      {{ }); }}
    </tbody>
  </table>
</script>
  

